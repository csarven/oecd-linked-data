<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE LIMES SYSTEM "limes.dtd">
<LIMES>
    <PREFIX>
        <NAMESPACE>http://www.w3.org/1999/02/22-rdf-syntax-ns#</NAMESPACE>
        <LABEL>rdf</LABEL>
    </PREFIX>
    <PREFIX>
        <NAMESPACE>http://www.w3.org/2000/01/rdf-schema#</NAMESPACE>
        <LABEL>rdfs</LABEL>
    </PREFIX>
    <PREFIX>
        <NAMESPACE>http://xmlns.com/foaf/0.1/</NAMESPACE>
        <LABEL>foaf</LABEL>
    </PREFIX>    
    <PREFIX>
        <NAMESPACE>http://www.w3.org/2002/07/owl#</NAMESPACE>
        <LABEL>owl</LABEL>
    </PREFIX>
    <PREFIX>
        <NAMESPACE>http://ecb.270a.info/code/0.1/</NAMESPACE>
        <LABEL>code</LABEL>
    </PREFIX>
    <PREFIX>
        <NAMESPACE>http://ecb.270a.info/classification/</NAMESPACE>
        <LABEL>classification</LABEL>
    </PREFIX>
    <PREFIX>
        <NAMESPACE>http://dbpedia.org/ontology/</NAMESPACE>
        <LABEL>dbpedia</LABEL>
    </PREFIX>
    <PREFIX>
        <NAMESPACE>http://purl.org/dc/terms/</NAMESPACE>
        <LABEL>dc</LABEL>
    </PREFIX>
    <PREFIX>
        <NAMESPACE>http://purl.org/linked-data/cube#</NAMESPACE>
        <LABEL>qb</LABEL>
    </PREFIX>
    <PREFIX>
        <NAMESPACE>http://dbpedia.org/ontology/</NAMESPACE>
        <LABEL>dbo</LABEL>
    </PREFIX>
    <PREFIX>
        <NAMESPACE>http://www.w3.org/2004/02/skos/core#</NAMESPACE>
        <LABEL>skos</LABEL>
    </PREFIX>

    <SOURCE>
        <ID>oecd-location</ID>
        <ENDPOINT>http://oecd.270a.info/sparql</ENDPOINT>
        <GRAPH>http://oecd.270a.info/graph/meta</GRAPH>
        <VAR>?x</VAR>
        <PAGESIZE>-1</PAGESIZE>
        <RESTRICTION>?x a skos:Concept</RESTRICTION>
        <RESTRICTION>?x skos:topConceptOf ?codeList</RESTRICTION>
        <RESTRICTION>FILTER(regex(str(?codeList), '_LOCATION'))</RESTRICTION>
        <PROPERTY>skos:prefLabel AS lowercase->nolang</PROPERTY>
    </SOURCE>

    <TARGET>
        <ID>imf-location</ID>
        <ENDPOINT>http://imf.270a.info/sparql</ENDPOINT>
        <GRAPH>http://imf.270a.info/graph/meta</GRAPH>
        <VAR>?y</VAR>
        <PAGESIZE>-1</PAGESIZE>
        <RESTRICTION>?y a skos:Concept</RESTRICTION>
        <RESTRICTION>?y skos:topConceptOf &lt;http://imf.270a.info/code/1.0/CL_AREA&gt;</RESTRICTION>
        <PROPERTY>skos:prefLabel AS lowercase->nolang</PROPERTY>
    </TARGET>


    <METRIC>trigrams(x.skos:prefLabel, y.skos:prefLabel)</METRIC>

    <ACCEPTANCE>
        <THRESHOLD>0.9</THRESHOLD>
        <FILE>oecd-imf.location.trigrams.accept.nt</FILE>
        <RELATION>&lt;http://www.w3.org/2004/02/skos/core#exactMatch&gt;</RELATION>
    </ACCEPTANCE>
    <REVIEW>
        <THRESHOLD>0.5</THRESHOLD>
        <FILE>oecd-imf.location.trigrams.review.nt</FILE>
        <RELATION>&lt;http://www.w3.org/2004/02/skos/core#exactMatch&gt;</RELATION>
    </REVIEW>

    <EXECUTION>Simple</EXECUTION>
    <GRANULARITY>16</GRANULARITY>
    <OUTPUT>N3</OUTPUT>
</LIMES>
